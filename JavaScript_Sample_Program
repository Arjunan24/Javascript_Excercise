scnerio 23
const books = [
    { title: "C++", author: "Bjarne" },
    { title: "Java", author: "James" },
    { title: "Python", author: "Guido" },
    { title: "Java", author: "James" },
];

let uniqueArr = books.filter((obj,index)=>{
    return index === books.findIndex(item=> obj.title == item.title)
})
console.log(uniqueArr)

output:
[
  { title: 'C++', author: 'Bjarne' },
  { title: 'Java', author: 'James' },
  { title: 'Python', author: 'Guido' }
]
