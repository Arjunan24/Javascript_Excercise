scnerio:18 check the value is prime number or not
function isPrime(n) { 
        // Corner case 
        if (n <= 1) 
            return false; 
  
        // Check from 2 to n-1 
        for (let i = 2; i < n; i++) 
            if (n % i == 0) 
                return false; 
  
        return true; 
    } 
  
    // Driver Code 
  
    isPrime(13)  
        ? console.log("true")  
        : console.log("false");

output:
true
---------------------------------------------------
