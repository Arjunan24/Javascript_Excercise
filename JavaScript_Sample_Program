scnerio :32
let arr =[3,2,1,3,44,5,5,3,44,5,44,1],count =0,minCounter=0
let max = Math.max(...arr)
let min=Math.min(...arr)

console.log(max,min)

//Maximum value and minimum count
arr.map(item=>{
    if(item == max){
      count++
    }
    if(item ==  min){
        minCounter++
    }
    
})
console.log(count,minCounter)

output:
44 1
3 2




